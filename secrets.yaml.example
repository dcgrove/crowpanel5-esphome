# ESPHome Secrets
# Copy this file to secrets.yaml and fill in your values
# DO NOT commit secrets.yaml to version control

wifi_ssid: "YourWiFiNetwork"
wifi_password: "YourWiFiPassword"

# API encryption key (generate with: python3 -c "import secrets, base64; print(base64.b64encode(secrets.token_bytes(32)).decode())")
api_pw: "REPLACE_WITH_GENERATED_KEY"

# OTA update password
ota_pw: "your-ota-password"

# Fallback hotspot password
fallback_pw: "fallback-password"
